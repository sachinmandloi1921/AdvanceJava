package collection;

import java.util.HashMap;

public class hashmap {
	public static void main(String[] args) {
		HashMap m = new HashMap();
		m.put("1", "One");
		m.put("2", "Two");
		m.put("3", "Three");
		
		int size = m.size();
		String value = (String)m.get("3");
		m.remove("1");
		System.out.println("m");
	}

}
