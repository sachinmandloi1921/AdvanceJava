package in.co.Deepraj;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;

import in.co.sachin.Regbean;

public class loginmodel extends bean {
public void add(bean b) throws Exception {
		
		Class.forName("com.mysql.jdbc.Driver");
		
		Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/deepraj","root","root");
		conn.setAutoCommit(false);
		PreparedStatement ps=conn.prepareStatement("insert into chouhan values(?,?)");
		
		ps.setString(1,b.getLogin());
		ps.setString(2,b.getPassword());
		
		int i = ps.executeUpdate();

		conn.commit();

		System.out.println(i + ":Record Inserted");

		ps.close();
		conn.close();

		

}

}
